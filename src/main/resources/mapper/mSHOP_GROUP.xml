<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Shop_Group">

<resultMap id="ShopGroupResultMap" type="com.osd.web.app.dto.Shop_GroupDto">

	<result column="GROUP_ID" property="group_id"/>
	<result column="GROUP_NAME" property="group_name"/>
	<result column="GROUP_SORT" property="group_sort"/>
	<result column="GROUP_USE" property="group_use"/>

</resultMap>
		 
<select id="selectAll" resultMap="ShopGroupResultMap">
	select * from shop_group
</select>

<insert id="insert" parameterType="Shop_GroupDto">
	insert into shop_group
	values
		(#{group_id}, #{group_name}, #{group_sort}, #{group_use})
</insert>
		
</mapper>